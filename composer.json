{
  "name": "tatevikgr/rss-bundle",
  "type": "symfony-bundle",
  "license": "AGPL-3.0-or-later",
  "description": "Symfony bundle for building recurring campaigns from RSS/Atom feeds for phpList4",
  "keywords": ["rss", "atom", "newsletter", "phplist", "symfony-bundle"],
  "require": {
    "php": ">=8.1",
    "debril/feed-io": "^6.0",
    "doctrine/orm": "^3.5",
    "symfony/framework-bundle": "^6.0 || ^7.0",
    "symfony/messenger": "^6.0 || ^7.0",
    "twig/twig": "^3.0",
    "symfony/http-client": "^6.4",
    "doctrine/doctrine-bundle": "^2.16",
    "doctrine/dbal": "^3.6"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^6.0 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "TatevikGr\\RssBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TatevikGr\\RssBundle\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Tatevik Grigoryan",
      "email": "tatevikg1@gmail.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
